proc setup {} {
    save_env
}; # setup()


proc teardown {} {
    assert_env_unmodified
}; # teardown()


setup


assert_complete_any "dpkg --c"


sync_after_int


    # Build list of installed packages
if {[assert_exec {dpkg --get-selections | command grep \[\[:space:\]\]install$ | cut -f1} packages]} {
    assert_complete $packages "dpkg -L "
}; # if


sync_after_int


teardown
