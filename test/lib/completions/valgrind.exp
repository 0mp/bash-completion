proc setup {} {
    save_env
}


proc teardown {} {
    assert_env_unmodified
}


setup


# b: assuming we have at least bash that starts with b in $PATH
assert_complete_any "valgrind b"
sync_after_int

assert_complete_any "valgrind -"
sync_after_int

assert_complete_any "valgrind --tool="
sync_after_int


teardown
