# bash completion for sysctl

have sysctl || return

_sysctl()
{
    local cur prev words cword
    _init_completion || return

    COMPREPLY=( $( compgen -W \
        "$( PATH="$PATH:/sbin" sysctl -N -a 2>/dev/null )" -- "$cur" ) )

    return 0
} &&
complete -F _sysctl sysctl

