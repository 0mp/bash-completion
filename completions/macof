# macof completion

_macof()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -i)
            _interfaces
            return 0
            ;;
    esac


    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '$( _parse_usage "$1" )' -- "$cur" ) )
    fi

} &&
complete -F _macof macof

