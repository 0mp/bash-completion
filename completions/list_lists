# mailman list_lists completion

_list_lists()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '--advertised --virtual-host-overview \
            --bare --help' -- "$cur" ) )
    fi

} &&
complete -F _list_lists list_lists

