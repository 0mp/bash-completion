# autorpm(8) completion

have autorpm &&
_autorpm()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	COMPREPLY=( $( compgen -W '--notty --debug --help --version \
				   auto add fullinfo info help install list \
				   remove set' -- "$cur" ) )

} &&
complete -F _autorpm autorpm

