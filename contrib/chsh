# chsh(1) completion

_chsh()
{
	local cur prev

	COMPREPLY=()
	cur=`_get_cword`
	prev=${COMP_WORDS[COMP_CWORD-1]}

	if [ "$prev" = "-s" ]; then
		_shells
	else
		COMPREPLY=( $( compgen -u -- "$cur" ) )
	fi

	return 0
}
complete -F _chsh chsh

