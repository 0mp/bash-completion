# bash completion for sysctl

have sysctl &&
_sysctl()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	COMPREPLY=( $( compgen -W "$(sysctl -N -a 2>/dev/null)" -- "$cur" ) )

	return 0
} &&
complete -F _sysctl sysctl

