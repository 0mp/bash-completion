# bash completion for rrdtool

have rrdtool &&
_rrdtool ()
{
	cur=`_get_cword`
	COMPREPLY=( $( compgen -W 'create update updatev graph dump \
	                           restore last lastupdate first info \
	                           fetch tune resize xport' -- "$cur" ) )
} &&
complete -F _rrdtool rrdtool

