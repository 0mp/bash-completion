# bash completion for cups

have cancel &&
_cancel()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	COMPREPLY=( $( compgen -W "$( lpstat | cut -d' ' -f1 )" -- "$cur" ) )
} &&
complete -F _cancel $filenames cancel

# Local variables:
# mode: shell-script
# sh-basic-offset: 8
# sh-indent-comment: t
# indent-tabs-mode: t
# End:
# ex: ts=8 sw=8 noet filetype=sh
